    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "angular-resize-event-package": "^3.5.0",
    "apollo-angular": "^7.2.1",
    "apollo-dynamic": "^1.0.4",
    "apollo-dynamic-angular": "^1.0.3",
